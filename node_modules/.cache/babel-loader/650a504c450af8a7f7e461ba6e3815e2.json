{"ast":null,"code":"export default {\n  name: \"SeatProjection\",\n  props: {\n    electionValue: Number,\n    electionParty: String,\n    currentView: String,\n    flip: Boolean,\n    difference: Number\n  },\n  data() {\n    let partyHex = this.$t(this.currentView + \".\" + this.electionParty + \"-colour\");\n    let barWidth = this.electionValue / 338 * 100;\n    let flipBox = \"column\";\n    let additionalVal = this.difference;\n    if (this.flip) {\n      flipBox = \"column-reverse\";\n      additionalVal = this.electionParty;\n    }\n    console.log(window.innerWidth);\n    return {\n      canShowAdditional: window.innerWidth > 800,\n      additionalVal: additionalVal,\n      partyName: this.currentView + \".\" + this.electionParty,\n      barStyle: {\n        backgroundColor: partyHex\n      },\n      numStyle: {\n        color: partyHex\n      },\n      boxStyle: {\n        width: barWidth + \"%\",\n        flexDirection: flipBox\n      }\n    };\n  },\n  methods: {}\n};","map":{"version":3,"mappings":"AAiBA;EACAA;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA,uBACA,wDACA;IACA;IACA;IACA;IACA;MACAC;MACAC;IACA;IACAC;IACA;MACAC;MACAF;MACAG;MACAC;QACAC;MACA;MACAC;QACAC;MACA;MACAC;QACAC;QACAC;MACA;IACA;EACA;EACAC;AACA","names":["name","props","electionValue","electionParty","currentView","flip","difference","data","flipBox","additionalVal","console","canShowAdditional","partyName","barStyle","backgroundColor","numStyle","color","boxStyle","width","flexDirection","methods"],"sourceRoot":"src/components","sources":["SeatProjection.vue"],"sourcesContent":["<template>\n  <div class=\"seatBox\" :style=\"boxStyle\">\n    <div class=\"colouredBar\" :style=\"barStyle\"></div>\n    <div class=\"electionNum\" :style=\"numStyle\">\n      {{ electionValue }}\n      <span v-if=\"electionValue > 10 && canShowAdditional\" class=\"additional\"\n        ><span v-if=\"!flip && additionalVal > 0\">+</span>{{ additionalVal }}\n        <span v-if=\"!flip\"\n          >seat<span v-if=\"additionalVal > 1 || additionalVal < -1\"\n            >s</span\n          ></span\n        ></span\n      >\n    </div>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"SeatProjection\",\n  props: {\n    electionValue: Number,\n    electionParty: String,\n    currentView: String,\n    flip: Boolean,\n    difference: Number,\n  },\n  data() {\n    let partyHex = this.$t(\n      this.currentView + \".\" + this.electionParty + \"-colour\"\n    );\n    let barWidth = (this.electionValue / 338) * 100;\n    let flipBox = \"column\";\n    let additionalVal = this.difference;\n    if (this.flip) {\n      flipBox = \"column-reverse\";\n      additionalVal = this.electionParty;\n    }\n    console.log(window.innerWidth);\n    return {\n      canShowAdditional: window.innerWidth > 800,\n      additionalVal: additionalVal,\n      partyName: this.currentView + \".\" + this.electionParty,\n      barStyle: {\n        backgroundColor: partyHex,\n      },\n      numStyle: {\n        color: partyHex,\n      },\n      boxStyle: {\n        width: barWidth + \"%\",\n        flexDirection: flipBox,\n      },\n    };\n  },\n  methods: {},\n};\n</script>\n<style>\n.colouredBar {\n  height: 25px;\n  border-radius: 5px;\n}\n.seatBox {\n  display: flex;\n  /* flex-direction: column-reverse; */\n  margin: 2px;\n}\n.electionNum {\n  font-weight: bold;\n  font-size: 20px;\n  text-align: left;\n}\n.additional {\n  font-weight: 400;\n  font-size: 13px;\n  color: #2c3e50;\n}\n@media only screen and (max-width: 800px) {\n  .electionNum {\n    font-size: 15px;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}